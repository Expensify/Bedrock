# Replace <username> with your macOS username
CompileFlags:
  CompilationDatabase: "cmake"
  Add:
    - -g
    - -std=c++20
    - -O0
    - -I/Users/<username>/Expensidev/Bedrock
    - --include=/Users/<username>/Expensidev/Bedrock/libstuff/SQResult.h
    - --include=/Users/<username>/Expensidev/Bedrock/libstuff/SRandom.h
    - --include=/Users/<username>/Expensidev/Bedrock/libstuff/SData.h
    - --include=/Users/<username>/Expensidev/Bedrock/sqlitecluster/SQLitePool.h
    - -I/Users/<username>/Expensidev/Bedrock/mbedtls/include
    - -DUSE_OS_TZDB=1
    - -Wall
    - -Werror
    - -Wformat-security
    - -Wno-error=deprecated-declarations
    - -Wno-unqualified-std-cast-call
    - -Wno-sign-conversion
    - -Wno-inconsistent-missing-override
    - -fPIC
